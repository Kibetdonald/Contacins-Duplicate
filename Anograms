class Solution:
    
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        # For it to be a perfect anogram both strings should have the same length
        # We start by checking if they have the same length if not to return False
        # So if length of S is not equal to the length of T return false and exit
      
        if len(s) != len(t):
            return False

        # Create two Hashmaps
        countS = {},
        countT = {}
        # Loop through through string S
        for i in range(len(s)):
            countS[s[i]] = 1 + countS.get(s[i], 0)
            countT[t[i]] = 1 + countT.get(t[i], 0)
        return countS == countT
